@Article{lasky_etal_2012,
  Title                    = {{Characterizing genomic variation of Arabidopsis thaliana: the roles of geography and climate}},
  Author                   = {Lasky, Jesse R. and Des Marais, David L. and McKAY, John K. and Richards, James H. and Juenger, Thomas E. and Keitt, Timothy H.},
