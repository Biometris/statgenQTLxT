@Article{valdar_Holmes_Mott_Flint_2009,
  Title                    = {Mapping in Structured Populations by Resample Model Averaging},
  Author                   = {Valdar, William and Holmes, Christopher C. and Mott, Richard and Flint, Jonathan},
