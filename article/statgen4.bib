@Article{Wu_etal_2009,
  Title                    = {Genome-wide association analysis by lasso penalized logistic regression},
  Author                   = {Wu, Tong Tong and Chen, Yi Fang and Hastie, Trevor and Sobel, Eric and Lange, Kenneth},
