@Article{citeulike_3015069,
  Title                    = {{Bayesian Survival Analysis in Genetic Association Studies.}},
  Author                   = {Tachmazidou, Ioanna and Andrew, Toby and Verzilli, Claudio J J. and Johnson, Michael R R. and Iorio, Maria De D.},
  Journal                  = {Bioinformatics (Oxford, England)},
