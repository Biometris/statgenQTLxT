  File                     = {korte_etal_2012.pdf:korte_etal_2012.pdf:PDF;korte_etal_2012_supplement.pdf:korte_etal_2012_supplement.pdf:PDF},
  Keywords                 = {mixed model, multiple phenotypes, population structure, quantitative genetics},
  Owner                    = {kruij025},
